<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN"
 "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleName</key>
    <string>San3a-ML-Runner</string>

    <key>CFBundleDisplayName</key>
    <string>San3a ML Runner</string>

    <key>CFBundleIdentifier</key>
    <string>com.san3a.academy.mlrunner</string>

    <key>LSMinimumSystemVersion</key>
    <string>11.0</string>

    <!-- CRITICAL: makes the app appear in front and accept keyboard input -->
    <key>LSBackgroundOnly</key>
    <false/>

    <!-- CRITICAL: tells macOS this is a full cocoa GUI app -->
    <key>LSUIElement</key>
    <false/>

    <!-- Optional but good -->
    <key>NSHighResolutionCapable</key>
    <true/>

</dict>
</plist>
